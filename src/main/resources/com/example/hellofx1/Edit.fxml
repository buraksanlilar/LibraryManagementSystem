<?xml version="1.0" encoding="UTF-8"?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.hellofx1.EditController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <TextField fx:id="title" layoutX="40.0" layoutY="20.0" promptText="Title"/>
        <TextField fx:id="subtitle" layoutX="40.0" layoutY="60.0" promptText="Subtitle"/>
        <Button fx:id="addButton" layoutX="40.0" layoutY="100.0" text="Add" onAction="#AddNewBook"/>
        <Button fx:id="resetButton" layoutX="120.0" layoutY="100.0" text="Reset" onAction="#ResetInput"/>
    </children>

</AnchorPane>
